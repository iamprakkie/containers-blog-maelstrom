{
    "AlarmName": "400 errors from ho11y app",
    "AlarmDescription": "Alarm from **ho11y app** - Error threshold breached for **HTTP status code 400**",
    "ActionsEnabled": true,
    "MetricName": "ho11y_total",
    "Namespace": "ho11yMetrics",
    "Statistic": "Sum",
    "Dimensions": [
        {
            "Name": "Namespace",
            "Value": "ho11y"
        },
        {
            "Name": "HTTPStatusCode",
            "Value": "400"
        }        
    ],
    "Period": 300,
    "EvaluationPeriods": 1,
    "Threshold": 15.0,
    "ComparisonOperator": "GreaterThanThreshold" 
}
